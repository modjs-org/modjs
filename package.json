{
  "name": "@modjs/monorepo",
  "version": "7.1.1",
  "workspaces": [
    "packages/core",
    "packages/helpers",
    "packages/icons",
    "packages/utils",
    "docs/modjs-docs-site/v7"
  ],
  "private": true,
  "keywords": [
    "modjs",
    "modjs core",
    "modjs icons",
    "react ui icons",
    "react ui components",
    "react",
    "ui",
    "frontend web development"
  ],
  "author": "Prashan Pudasaini",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modjs-org/modjs.git"
  },
  "scripts": {
    "clean:docs": "rm -rf ./node_modules ./package-lock.json ./docs/modjs-docs-site/v7/.next ./docs//modjs-docs-site/v7/node_modules ./docs//modjs-docs-site/v7/out",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./docs/modjs-docs-site/v7/.next ./docs//modjs-docs-site/v7/node_modules ./docs//modjs-docs-site/v7/out ./docs//modjs-docs-site/v7/package-lock.json ./packages/core/build ./packages/core/node_modules ./packages/helpers/build ./packages/helpers/node_modules ./packages/helpers/coverage ./packages/icons/build ./packages/icons/node_modules ./packages/utils/build ./packages/utils/coverage ./packages/utils/node_modules",
    "start:docs": "npm run dev --workspace docs",
    "test:docs": "npm run test:e2e:ui --workspace docs",
    "build:docs": "npm run build --workspace docs",
    "preDeploy:docs": "npm run preDeploy --workspace docs",
    "test:helpers": "npm run test:unit:ui --workspace @modjs/helpers",
    "build:helpers": "npm run build --workspace @modjs/helpers",
    "prePublish:helpers": "npm run prePublish --workspace @modjs/helpers",
    "publish:helpers": "npm run publish --workspace @modjs/helpers",
    "test:utils": "npm run test:component:ui --workspace @modjs/utils",
    "build:utils": "npm run build --workspace @modjs/utils",
    "prePublish:utils": "npm run prePublish --workspace @modjs/utils",
    "publish:utils": "npm run publish --workspace @modjs/utils",
    "test:icons": "npm run test:component:ui --workspace @modjs/icons",
    "build:icons": "npm run build --workspace @modjs/icons",
    "prePublish:icons": "npm run prePublish --workspace @modjs/icons",
    "publish:icons": "npm run publish --workspace @modjs/icons",
    "test:core": "npm run test:component:ui --workspace @modjs/core",
    "build:core": "npm run build --workspace @modjs/core",
    "prePublish:core": "npm run prePublish --workspace @modjs/core",
    "publish:core": "npm run publish --workspace @modjs/core",
    "build:all": "npm-run-all -s build:helpers build:utils build:icons build:core build:docs"
  },
  "dependencies": {
    "@modjs/core": "^7.1.0",
    "@modjs/helpers": "^7.0.0",
    "@modjs/icons": "^7.1.1",
    "@modjs/utils": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@cypress/code-coverage": "^3.12.39",
    "@eslint/js": "^9.3.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "babel-plugin-styled-components": "^2.1.4",
    "css-loader": "^7.1.2",
    "cypress": "^13.9.0",
    "eslint": "^8.57.0",
    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "vitest": "^1.6.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
