{
  "name": "@modjs/docs-site",
  "version": "7.0.0",
  "keywords": [
    "modjs"
  ],
  "author": "Prashan Pudasaini",
  "repository": {
    "type": "git",
    "url": "https://github.com/modjs-org/modjs.git"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "clean": "rm -rf ./out rm -rf ./.next",
