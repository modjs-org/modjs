    "@modjs/core": "file:../../../packages/core",
    "next": "^14.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "styled-components": "^6.1.11"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4"
